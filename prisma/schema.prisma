generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model taxis {
  id           String         @id @db.VarChar
  placa        String?        @db.VarChar
  trajectories trajectories[]
}

model trajectories {
  id         String   @db.VarChar
  taxi       taxis    @relation(fields: [id], references: [id])
  created_at DateTime @db.Timestamp(6)
  latitude   Float?
  longitude  Float?

  @@id([id, created_at])
}
